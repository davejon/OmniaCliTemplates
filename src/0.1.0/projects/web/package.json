{
  "name": "$namespace$",
  "version": "0.1.0",
  "scripts": {
    "omnia-before-build": "gulp omnia-before-build",
    "omnia-after-build": "gulp omnia-after-build"
  },
  "dependencies": {
    "@omnia/fx": "0.4.125-vnext",
    "@omnia/fx-sp": "0.4.125-vnext",
    "@omnia/fx-spfx": "0.4.125-vnext",
    "gulp": "3.9.1",
    "gulp-cli": "2.0.1"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx|jsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "(@omnia/fx/)": "<rootDir>/__mocks__/index.ts"
    },
    "testRegex": "(/Resources/.*|\\.(test|spec))\\.(test.ts|test.tsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/Resources/Dist"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node"
  }  
}
